{
    "name": "anvargear/laravel-states-col",
    "description": "Laravel Departaments and Cities Colombia is a bundle for Laravel, providing Almost ISO 3166_3, Dane Code and Capital.",
    "type": "laravel-library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrés Felipe Vásquez",
            "email": "avasquez@anvargear.co",
            "homepage" : "https://www.anvargear.co/"
        }
    ],
    "keywords": [
        "laravel",
        "states",
        "colombia",
        "departamentos",
        "ciudades"
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "support": {
        "email": "avasquez@anvargear.co",
        "wiki": "https://github.com/AnvarGear/laravel-states-col/wiki"
    },
    "autoload": {
        "psr-4": {
            "AnvarGear\\States\\": "src/"
        },
        "classmap" : [
            "src/commands"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AnvarGear\\States\\Tests\\": "tests/"
        }
    },
    "minimum-stability" : "dev",
    "support" : {
        "source" : "https://github.com/AnvarGear/laravel-states-col",
        "issues" : "https://github.com/AnvarGear/laravel-states-col/issues"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnvarGear\\States\\StatesServiceProvider"
            ],
            "aliases": {
                "StatesCol": "AnvarGear\\States\\StatesFacade"
            }
        }
    }
}
